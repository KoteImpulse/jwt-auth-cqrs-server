{
	"name": "jwt-auth-cqrs-server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint:ts": "eslint \"**/*.{ts}\"",
		"lint:ts:fix": "eslint \"**/*.{ts}\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:unit": "jest --config ./config/jest/jest.unit.config.js",
		"test:e2e": "cross-env NODE_ENV=test jest --config ./config/jest/jest.e2e.config.js",
		"test:all": "jest --config ./config/jest/jest.all.config.js",
		"db:push": "prisma db push",
		"db:gen": "prisma generate",
		"db:studio": "prisma studio"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/cqrs": "^11.0.3",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/schedule": "^6.0.0",
		"@nestjs/swagger": "^11.2.0",
		"@prisma/client": "^6.14.0",
		"argon2": "^0.44.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"cookie-parser": "^1.4.7",
		"jsonwebtoken": "^9.0.2",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"prisma": "^6.14.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/cookie-parser": "^1.4.9",
		"@types/express": "^5.0.0",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^22.10.7",
		"@types/passport-jwt": "^4.0.1",
		"@types/passport-local": "^1.0.38",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^8.40.0",
		"@typescript-eslint/parser": "^8.40.0",
		"cross-env": "^10.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-prettier": "^5.2.2",
		"eslint-plugin-unused-imports": "^4.2.0",
		"globals": "^16.0.0",
		"jest": "^30.0.0",
		"prettier": "^3.4.2",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
